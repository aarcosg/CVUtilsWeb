{
    "require": {
        "php": ">=5.4.0",
        "ext-mbstring": "*",
        "illuminate/database": "5.1.41",
        "league/flysystem": "^1.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/pagination": "5.1.41",
        "landish/pagination": "^1.3"
    },
    "require-dev": {
        "ext-fileinfo": "*",
        "phpunit/phpunit": "5.6.2",
        "mockery/mockery": "~0.9",
        "predis/predis": "~1.0",
        "tedivm/stash": "~0.12.0",
        "league/phpunit-coverage-listener" : "~1.1",
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "League\\Flysystem\\": "src/"
        },
        "classmap": [
            "models"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Flysystem\\Stub\\": "stub/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
